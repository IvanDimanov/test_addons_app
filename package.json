{
  "name": "test_addons_app",
  "version": "1.1.0",
  "description": "Testing a tech stack of MongoDB, Express, Node, Backbone and Marionette",
  "readme": "README.md",
  "author": "Ivan Dimanov <socialhub@idimanov.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IvanDimanov/test_addons_app"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production babel-node babel_index.js",
    "lint": "standard",
    "create-db-files": "babel-node ./data/create-db-files.js",
    "start-db": "(mongod --config=./data/configure_mongod.txt &); sleep 2",
    "initial-db-start": "npm run create-db-files && npm run start-db",
    "set-basic-data": "mongorestore ./data/dump",
    "set-features-data": "babel-node ./data/recreate-features-collection.js",
    "set-db-data": "npm run set-basic-data && npm run set-features-data",
    "initial-build": "npm install && bower install && npm run initial-db-start && npm run build-frontend",
    "build-frontend": "webpack",
    "develop-frontend": "webpack-dev-server --content-base=./frontend-app/public",
    "develop-backend": "cross-env NODE_ENV=production pm2 start babel_index.js --interpreter=babel-node --name=test_addons_app --log=screen.log",
    "postdevelop-backend": "(babel-node .watch.js --watch=./backend-app --command=\"pm2 restart test_addons_app\" >> .watch.log &)",
    "develop-backend-stop": "pm2 stop test_addons_app",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "engines": {
    "node": ">=4.0"
  },
  "standard": {
    "ignore": [
      ".git/",
      "node_modules/",
      "data/",
      "notes/",
      "config/index.js",
      "utils.js",
      "frontend-app/assets/vendor",
      "frontend-app/assets/public"
    ]
  },
  "dependencies": {
    "babel": "6.0.15",
    "babel-cli": "6.9.0",
    "babel-core": "6.10.4",
    "babel-eslint": "4.1.7",
    "babel-loader": "6.2.0",
    "babel-plugin-transform-es2015-arrow-functions": "6.0.14",
    "babel-plugin-transform-es2015-block-scoped-functions": "6.0.14",
    "babel-plugin-transform-es2015-block-scoping": "6.9.0",
    "babel-plugin-transform-es2015-classes": "6.1.2",
    "babel-plugin-transform-es2015-computed-properties": "6.0.14",
    "babel-plugin-transform-es2015-constants": "6.0.15",
    "babel-plugin-transform-es2015-destructuring": "6.0.18",
    "babel-plugin-transform-es2015-for-of": "6.0.14",
    "babel-plugin-transform-es2015-function-name": "6.0.14",
    "babel-plugin-transform-es2015-literals": "6.0.18",
    "babel-plugin-transform-es2015-modules-commonjs": "6.1.3",
    "babel-plugin-transform-es2015-object-super": "6.0.14",
    "babel-plugin-transform-es2015-parameters": "6.0.18",
    "babel-plugin-transform-es2015-shorthand-properties": "6.0.14",
    "babel-plugin-transform-es2015-spread": "6.0.14",
    "babel-plugin-transform-es2015-sticky-regex": "6.0.18",
    "babel-plugin-transform-es2015-template-literals": "6.0.14",
    "babel-plugin-transform-es2015-typeof-symbol": "6.1.2",
    "babel-plugin-transform-es2015-unicode-regex": "6.0.18",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.1.2",
    "babel-preset-stage-1": "6.1.2",
    "babel-preset-stage-2": "6.1.2",
    "babel-preset-stage-3": "6.1.2",
    "body-parser": "1.18.2",
    "bower": "1.7.5",
    "chokidar": "2.0.0",
    "cross-env": "1.0.7",
    "css-loader": "0.23.0",
    "eslint": "2.1.0",
    "express": "4.16.0",
    "express-winston": "0.4.1",
    "extract-text-webpack-plugin": "0.9.1",
    "mongodb": "2.2.13",
    "node-sass": "3.5.1",
    "nodemailer": "1.10.0",
    "pm2": "2.10.0",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "validator": "9.4.1",
    "webpack": "2.2.0",
    "webpack-dev-server": "1.14.0",
    "winston": "2.1.1",
    "yargs": "3.31.0",
    "snyk": "^1.316.2"
  },
  "devDependencies": {
    "standard": "5.4.1"
  },
  "snyk": true
}